{
  "compilerOptions": {
		"baseUrl": "./src",
    "moduleResolution": "node",
    "target": "es2019",
    "lib": ["ES2019"],
    "outDir": "out",
    "sourceMap": true,
    "strict": true,
    "rootDir": "src",
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules",
  ]
}
